      "get": {
        "tags": [
          "Public Installation Viability"
        ],
        "summary": "Get nearest condominium point location by address",
        "operationId": "checkCondominiumPointUsingGET",
        "parameters": [
          {
            "name": "apikey",
            "in": "header",
            "description": "Gateway Token",
            "required": true,
            "type": "string",
            "default": "HzAyvWlatOs6VhqyLTOQqVN4TrpycTSGDQ7M94ZXsqPOmArT4A8HzS5CzOxTqza"
          },
          {
            "name": "blockFullName",
            "in": "query",
            "description": "Condominium block full name",
            "required": false,
            "type": "string"
          },
          {
            "name": "city",
            "in": "query",
            "description": "Address city name",
            "required": false,
            "type": "string",
            "x-example": "Carmo"
          },
          {
            "name": "condominiumFullName",
            "in": "query",
            "description": "Condominium full name",
            "required": false,
            "type": "string"
          },
          {
            "name": "country",
            "in": "query",
            "description": "Address country",
            "required": false,
            "type": "string",
            "x-example": "Brazil"
          },
          {
            "name": "district",
            "in": "query",
            "description": "Address district name",
            "required": false,
            "type": "string",
            "x-example": "Centro"
          },
          {
            "name": "number",
            "in": "query",
            "description": "Address number",
            "required": false,
            "type": "string",
            "x-example": "1"
          },
          {
            "name": "state",
            "in": "query",
            "description": "Address state",
            "required": false,
            "type": "string",
            "x-example": "RJ"
          },
          {
            "name": "streetName",
            "in": "query",
            "description": "Address street name",
            "required": false,
            "type": "string",
            "x-example": "Rua F"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/InstallationViabilityCondominiumDto"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
